{
  "name": "aegis-api",
  "version": "1.0.0",
  "description": "Fastify server for utility api's used in kanalabs",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "build-docker": "docker build -t kana-utils .",
    "start-docker": "docker run -p 3000:3006 -d kana-utils",
    "prepare": "husky install"
  },
  "author": "Matschik <mathieu.schimmerling@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.29.1",
    "@commitlint/config-conventional": "^18.4.4",
    "@fastify/cookie": "^10.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/session": "^11.0.1",
    "axios": "1.6.3",
    "axios-retry": "^4.0.0",
    "commitlint": "^18.6.0",
    "dotenv": "16.3.1",
    "fastify": "^5.0.0",
    "i": "0.3.7",
    "lodash": "4.17.21",
    "mongoose": "^8.6.3",
    "node-cache": "5.1.2",
    "npm": "10.2.5",
    "snarkjs": "^0.7.4",
    "twitter-api-v2": "^1.17.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "20.10.5",
    "@types/snarkjs": "^0.7.8",
    "cross-env": "7.0.3",
    "fastify-tsconfig": "2.0.0",
    "husky": "^8.0.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
